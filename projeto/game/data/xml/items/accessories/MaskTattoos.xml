	<!-- Adicionar +250 em Defesas, e resistencia a todos os elementos -->
	<item id="9548" type="Armor" name="Salem Mask">
		<set name="default_action" val="equip" />
		<set name="bodypart" val="face" />
		<set name="material" val="WOOD" />
		<set name="weight" val="10" />
		<set name="price" val="0" />
		<set name="is_tradable" val="true" />
		<set name="is_dropable" val="true" />
		<set name="is_sellable" val="false" />
		<set name="is_depositable" val="true" />		
		<!-- <set name="item_skill" val="3599-1" />-->
		<set name="is_oly_restricted" val="true" />
		<for>
			<add order="0x40" stat="pDef" val="250" />
			<add order="0x40" stat="mDef" val="250" />
			<mul order="0x30" stat="darkRes" val="30" />
			<mul order="0x30" stat="holyRes" val="30" />
			<mul order="0x30" stat="waterRes" val="20" />
			<mul order="0x30" stat="fireRes" val="20" />
			<mul order="0x30" stat="windRes" val="20" />
			<mul order="0x30" stat="earthRes" val="20" />
		</for>
	</item>
